
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
    string baseUrl = Request.Url.Scheme + "://" + Request.Url.Authority +
Request.ApplicationPath.TrimEnd('/');
}


<div class="row">
    <div class="col-lg-6">
        <div class="card mb-4">
            <div class="card-header">
                <i class="fas fa-chart-bar me-1"></i>
                Call Total 
            </div>
            <div class="card-body"><canvas id="myBarChart" width="100%" height="65"></canvas></div>
            <div class="card-footer small text-muted"></div>
        </div>
    </div>
    <div class="col-lg-6">
        <div class="card mb-4">
            <div class="card-header row">
                <div class="col-md-9 col-xs-9">
                    <i class="fas fa-chart-pie me-1"></i>
                    Call Perfomance (CALL VS TARGET)

                </div>
                <div class="col-md-3 col-xs-3">
                    <b class="text-center MonthName"></b>
                </div>
                

            </div>
            <div class="card-body"><canvas id="myPieChart" width="100%" height="50"></canvas></div>
            <h3 class="text-center percentage">50%</h3>
            <div class="card-footer small text-muted"></div>
        </div>
    </div>
</div>



<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.8.0/Chart.min.js" crossorigin="anonymous"></script>
<script src="@baseUrl/bootstrapadmin/demo/chart-area-demo.js"></script>
<script src="~/Scripts/CustomerApps/Dashboard.js">
   

</script>
